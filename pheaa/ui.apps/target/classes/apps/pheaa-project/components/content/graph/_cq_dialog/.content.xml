<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Lone Summary"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        granite:class="core-wcm-list-v1"
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <listSettings
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Loan Detail"
                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <lone
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/numberfield"
                                        fieldDescription="Total Lone Amount"
                                        fieldLabel="Lone Amount"
                                        name="./lone"/>
                                    <paid
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/numberfield"
                                        fieldDescription="Amount Paid"
                                        fieldLabel="Amount Paid"
                                        name="./paid"/>
                                    <interestrates
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/numberfield"
                                        fieldDescription="Interest Rates"
                                        fieldLabel="Interest Rates"
                                        name="./interestrates"/>
                                </items>
                            </column>
                        </items>
                    </listSettings>
                    <itemSettings
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Loan Detail Fields "
                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <column1
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <title1
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Your Balance"
                                        fieldLabel="Your Balance"
                                        name="./title1"/>
                                    <linkItems
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                        checked="${not empty cqDesign.linkItems ? cqDesign.linkItems : false}"
                                        fieldDescription="Link items to the corresponding page"
                                        name="./linkItem1"
                                        text="Link Items"
                                        uncheckedValue="false"
                                        value="true"/>
                                </items>
                            </column1>
                            <column2
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <title1
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Paid as amount "
                                        fieldLabel="Paid Amount"
                                        name="./title2"/>
                                    <linkItems
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                        checked="${not empty cqDesign.linkItems ? cqDesign.linkItems : false}"
                                        fieldDescription="Link items to the corresponding page"
                                        name="./linkItem2"
                                        text="Link Items"
                                        uncheckedValue="false"
                                        value="true"/>
                                </items>
                            </column2>
                            <column3
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <title1
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Total Balance"
                                        fieldLabel="Total Balance"
                                        name="./title3"/>
                                    <linkItems
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                        checked="${not empty cqDesign.linkItems ? cqDesign.linkItems : false}"
                                        fieldDescription="Link items to the corresponding page"
                                        name="./linkItem3"
                                        text="Link Items"
                                        uncheckedValue="false"
                                        value="true"/>
                                </items>
                            </column3>
                            <column4
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <title1
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Principle Balance"
                                        fieldLabel="Principle Balance"
                                        name="./title4"/>
                                    <linkItems
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                        checked="${not empty cqDesign.linkItems ? cqDesign.linkItems : false}"
                                        fieldDescription="Link items to the corresponding page"
                                        name="./linkItem4"
                                        text="Link Items"
                                        uncheckedValue="false"
                                        value="true"/>
                                </items>
                            </column4>
                            <column5
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <title2
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Interest Balance "
                                        fieldLabel="Interest Balance "
                                        name="./title5"/>
                                    <linkItems
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                        checked="${not empty cqDesign.linkItems ? cqDesign.linkItems : false}"
                                        fieldDescription="Title item to the corresponding page"
                                        name="./linkItem5"
                                        text="Show Items"
                                        uncheckedValue="false"
                                        value="true"/>
                                </items>
                            </column5>
                        </items>
                    </itemSettings>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
